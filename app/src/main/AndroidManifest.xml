            android:theme="@style/uplacetheme" />
        <activity
            android:name=".mainMenu.ForumActivity"
            android:label="Forum"
            android:theme="@style/uplacetheme" />
        <activity
            android:name=".mainMenu.CariActivity"
            android:label="Pencarian"
            android:theme="@style/uplacetheme" />
        <activity android:name=".mainMenu.DanaSayaActivity" />
        <activity android:name=".Mainmenu" />
        <activity android:name=".mainMenu.sideMenu.PostActivity" />
        <activity android:name=".mainMenu.ProfileActivity" />
        <activity android:name=".mainMenu.BarangActivity" />
